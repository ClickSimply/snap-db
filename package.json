{
  "name": "snap-db",
  "version": "0.0.3",
  "description": "Simple & Blazing Fast Javascript/Webassembly key-value store",
  "main": "bin/index.js",
  "scripts": {
    "build-wasm": "cd src-wasm && ./build.sh --prod && cp ../src/db-index.js ../bin/db-index.js && cp ../src/db-index.wasm ../bin/db-index.wasm",
